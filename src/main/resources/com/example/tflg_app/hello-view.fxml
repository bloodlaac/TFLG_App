<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" minHeight="400.0" minWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tflg_app.AnalyzerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText" text="Анализируемая строка" textAlignment="JUSTIFY">
        <font>
            <Font name="Times New Roman" size="16.0"/>
        </font>
    </Label>
    <TextField fx:id="stringField"/>
    <Label text="Список ID">
        <font>
            <Font name="Times New Roman" size="16.0"/>
        </font>
    </Label>
    <TextField/>
    <Label text="Список CONST">
        <font>
            <Font name="Times New Roman" size="16.0"/>
        </font>
    </Label>
    <TextField/>
    <Label text="Ошибка">
        <font>
            <Font name="Times New Roman" size="16.0"/>
        </font>
    </Label>
    <TextField/>
</VBox>
